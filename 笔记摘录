    'share_code.extensionsItem.SpiderOpenCloseLogging': 100,
    'share_code.extensionsTime.Latencies': None,
    'scrapy.contrib.memusage.MemoryUsage': 50,
    'scrapy.contrib.memdebug.MemoryDebugger': 60
}

